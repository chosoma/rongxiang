<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="com.thingtek.config.clazz.ClazzConfig">
        <property name="unitClazzMap">
            <map>
                <entry key="1" value="com.thingtek.beanServiceDao.unit.entity.SF6UnitBean"/>
                <entry key="4" value="com.thingtek.beanServiceDao.unit.entity.DisUnitBean"/>
            </map>
        </property>
        <property name="dataClazzMap">
            <map>
                <entry key="1" value="com.thingtek.beanServiceDao.data.entity.SF6DataBean"/>
                <entry key="4" value="com.thingtek.beanServiceDao.data.entity.DisDataDao"/>
            </map>
        </property>
    </bean>
</beans>